{
  "name": "times-table-js",
  "version": "0.3.0",
  "description": "Interactive rendering of times tables",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "publish": "npm run clean && npm run build-prod && npm run gh-pages",
    "build": "webpack --config webpack.dev.js",
    "build-prod": "webpack --config webpack.prod.js",
    "build-prod-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "clean": "rimraf build/**/*",
    "tsc": "tsc --traceResolution",
    "gh-pages": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MathiasLengler/TimesTableJS.git"
  },
  "author": "Mathias Lengler",
  "license": "MIT",
  "dependencies": {
    "dat.gui": "^0.7.6",
    "stats.js": "^0.17.0",
    "three": "^0.109.0"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.5",
    "@types/stats": "^0.16.30",
    "babel-core": "^6.26.3",
    "babel-loader": "8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "3.2.0",
    "gh-pages": "2.1.1",
    "html-webpack-plugin": "3.2.0",
    "raw-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "scarlett-glslify-loader": "^1.0.3",
    "style-loader": "1.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "3.6.4",
    "webpack": "4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
