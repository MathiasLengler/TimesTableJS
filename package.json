{
  "name": "times-table-js",
  "version": "0.3.0",
  "description": "Interactive rendering of times tables",
  "main": "main.js",
  "scripts": {
    "start": "webpack-serve --config webpack.dev.js",
    "publish": "npm run clean && npm run pack-prod && npm run gh-pages",
    "pack": "webpack --config webpack.dev.js",
    "pack-prod": "webpack --config webpack.prod.js",
    "pack-profile": "webpack --profile --json > stats.json",
    "clean": "rimraf build/**/*",
    "tsc": "tsc --traceResolution",
    "gh-pages": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MathiasLengler/TimesTableJS.git"
  },
  "author": "Mathias Lengler",
  "license": "MIT",
  "dependencies": {
    "dat.gui": "^0.7.2",
    "stats.js": "^0.17.0",
    "three": "^0.92.0"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.0",
    "@types/stats": "^0.16.30",
    "@types/three": "^0.92.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "0.28.11",
    "gh-pages": "1.1.0",
    "html-webpack-plugin": "3.2.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "scarlett-glslify-loader": "^1.0.3",
    "style-loader": "0.21.0",
    "ts-loader": "^4.3.0",
    "typescript": "2.8.3",
    "webpack": "4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^1.0.2"
  }
}
