{
  "name": "times-table-js",
  "version": "0.3.0",
  "description": "Interactive rendering of times tables",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0",
    "publish": "npm run clean && npm run pack-prod && npm run gh-pages",
    "pack": "webpack",
    "pack-prod": "webpack -p",
    "pack-profile": "webpack --profile --json > stats.json",
    "clean": "rimraf build/**/*",
    "tsc": "tsc --traceResolution",
    "gh-pages": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MathiasLengler/TimesTableJS.git"
  },
  "author": "Mathias Lengler",
  "license": "MIT",
  "dependencies": {
    "@types/react-dom": "^0.14.23",
    "dat.gui": "github:dataarts/dat.gui",
    "postcss": "^5.2.16",
    "postcss-cssnext": "^2.9.0",
    "postcss-each": "^0.9.3",
    "postcss-import": "^9.1.0",
    "postcss-mixins": "^5.4.1",
    "postcss-modules-values": "^1.2.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-toolbox": "^2.0.0-beta.7",
    "stats.js": "^0.17.0",
    "three": "^0.84.0"
  },
  "devDependencies": {
    "@types/dat-gui": "^0.6.2",
    "@types/react": "^15.0.15",
    "@types/stats": "^0.16.30",
    "awesome-typescript-loader": "^3.0.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.24.1",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "typescript": "2.1.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-glsl-loader": "^1.0.1"
  }
