{
  "name": "times-table-js",
  "version": "0.3.0",
  "description": "Interactive rendering of times tables",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf .publish/**/* build/**/* dist/**/*",
    "start": "webpack-dev-server --config webpack/webpack.config.js --content-base build/",
    "pack": "webpack --config webpack/webpack.config.js",
    "pack-prod": "webpack -p --config webpack/webpack.config.js",
    "pack-profile": "webpack --config webpack/webpack.config.js --profile --json > stats.json",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MathiasLengler/TimesTableJS.git"
  },
  "author": "Mathias Lengler",
  "license": "MIT",
  "dependencies": {
    "dat.gui": "github:dataarts/dat.gui",
    "stats.js": "^0.17.0",
    "three": "^0.84.0"
  },
  "devDependencies": {
    "@types/dat-gui": "^0.6.2",
    "@types/stats": "^0.16.30",
    "awesome-typescript-loader": "^3.0.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.24.1",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "typescript": "2.1.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-glsl-loader": "^1.0.1"
  }
}
